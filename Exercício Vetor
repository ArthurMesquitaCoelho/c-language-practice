#include <stdio.h>

#define TAMANHO 5

void main()
{
    float nota[TAMANHO], soma, media;
    int i;
    
    for(i = 0; i < TAMANHO; i++)
    {
        printf("Digite uma nota: ");
        scanf("%f", &nota[i]);
    }
    soma = 0;
    for(i = 0; i < TAMANHO; i++)
        soma = soma + nota[i];
    media = soma / TAMANHO;
    printf("media = %.1f", media);
}
